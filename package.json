{
  "name": "swift-add-documentation",
	"displayName": "SwiftAddDocumentation",
	"description": "Generate documentation template for methods, classes etc.",
	"version": "0.0.1",
    "publisher": "FredAPackages",
    "icon": "icon.png",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
        "onCommand:swift-add-documentation.register"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "swift-add-documentation.register",
				"title": "SwiftAddDocumentation"
			}
		]
	},
    "keybindings": [
        {
            "command": "swift-add-documentation.register",
            "key": "Alt+Win+/",
            "mac": "Alt+Cmd+/",
            "when": "editorTextFocus"
        }
    ],
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	},
    "repository": {
        "type": "git",
        "url": "https://github.com/fappelman/swift-add-documentation.git"
    }
}
